{
    "name": "roave/roave-developer-tools",
    "description": "Developer Tools aimed at debugging/profiling/logging actions that are executed within a standard Zend\\Mvc application",
    "license": "MIT",
    "keywords": [
        "debug",
        "developer",
        "zendframework",
        "module"
    ],
    "homepage": "https://github.com/zendframework/ZendDeveloperTools",
    "authors": [
        {
            "name": "Marco Pivetta",
            "email": "ocramius@gmail.com",
            "homepage": "http://ocramius.github.io/"
        },
        {
            "name": "Roave LLC",
            "email": "info@roave.com",
            "homepage": "https://roave.com/"
        }
    ],
    "require": {
        "php":                         "~5.5",
        "zendframework/zendframework": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit":           "~4.0.0",
        "squizlabs/php_codesniffer": "1.5.*"
    },
    "autoload": {
        "psr-0": {
            "Roave\\DeveloperTools\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "RoaveTest\\DeveloperTools\\": "test/"
        }
    }
}
